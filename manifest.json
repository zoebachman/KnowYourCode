{
  "manifest_version": 2,

  "name": "Know Your Code",
  "description": "This extension shows the diversity statistics for top tech companies",
  "version": "1.0",

  "browser_action": {
    "default_icon": "KYC_icon_lg.png",
    "default_popup": "index.html",
    "default_title": "Know Your Code"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}